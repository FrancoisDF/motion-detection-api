{
  "name": "motion-detection-api",
  "version": "0.1.0",
  "description": "NodeJs API server to process images to extract person and send email/alert",
  "main": "src/index.ts",
  "scripts": {
    "start": "node -r ts-node/register --inspect ./src/index.ts",
    "dev": "nodemon",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -p . --fix",
    "format": "prettier \"src/**/*.ts\" -l \"warn\"",
    "format:fix": "prettier --write \"src/**/*.ts\" -l \"warn\"",
    "format:lint:fix": "prettier --write \"src/**/*.ts\" -l \"warn\" && npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/r0muald/motion-detection.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "expressjs",
    "server",
    "motioneye",
    "tensorflow",
    "human",
    "detection",
    "security",
    "camera"
  ],
  "author": "francois.delpech@sfr.fr",
  "license": "MIT",
  "homepage": "https://github.com/r0muald/motion-detection",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@tensorflow-models/coco-ssd": "^2.2.1",
    "@tensorflow/tfjs-node": "^3.3.0",
    "@types/nodemailer": "^6.4.1",
    "compression": "^1.7.4",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.17.0",
    "express-joi-validation": "^5.0.0",
    "helmet": "^4.4.1",
    "jpeg-js": "^0.4.4",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.5.0",
    "rpi-gpio": "^2.1.7"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "1.7.0",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "1.5.0",
    "@types/express": "^4.17.11",
    "@types/hapi__joi": "^17.1.6",
    "@types/helmet": "4.0.0",
    "@types/node": "^14.14.37",
    "eslint": "^7.24.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.2.4"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
