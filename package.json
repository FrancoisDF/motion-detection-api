{
  "name": "motioneye-detection",
  "version": "0.1.0",
  "description": "NodeJs API server to process images to extract person and send email/alert",
  "main": "src/index.ts",
  "scripts": {
    "start": "node -r ts-node/register --inspect ./src/index.ts",
    "dev": "nodemon",
    "build": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -p . --fix",
    "format": "prettier \"src/**/*.ts\" -l \"warn\"",
    "format:fix": "prettier --write \"src/**/*.ts\" -l \"warn\"",
    "format:lint:fix": "prettier --write \"src/**/*.ts\" -l \"warn\" && npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/r0muald/motioneye-detection.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "expressjs",
    "server",
    "motioneye",
    "tensorflow",
    "human",
    "detection",
    "security",
    "camera"
  ],
  "author": "francois.delpech@sfr.fr",
  "license": "MIT",
  "homepage": "https://github.com/r0muald/motioneye-detection",
  "dependencies": {
    "@hapi/joi": "^17.0.0",
    "@tensorflow-models/coco-ssd": "^2.0.1",
    "@tensorflow/tfjs-node": "^1.5.1",
    "@types/nodemailer": "^6.4.0",
    "compression": "^1.7.4",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.17.0",
    "express-joi-validation": "^4.0.3",
    "helmet": "^3.16.0",
    "jpeg-js": "^0.3.6",
    "nodemailer": "^6.4.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "1.0.1",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.1",
    "@types/hapi__joi": "^16.0.6",
    "@types/helmet": "0.0.45",
    "@types/node": "^13.1.4",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.17.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.17.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
